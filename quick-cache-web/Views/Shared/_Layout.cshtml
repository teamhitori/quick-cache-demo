<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - quick_cache_web</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" />
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@300;400;500&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
</head>
<body>
    @RenderBody()
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.min.js"></script>
    @await RenderSectionAsync("Scripts", required: false)

    <script type="module" src="@Configuration["static_url"]/polyfills.js"></script>
    <script type="module" src="@Configuration["static_url"]/main.js"></script>

    <environment exclude="Development">
        <link rel="stylesheet" href="@Configuration["static_url"]/styles.css" />
    </environment>
</body>
</html>
